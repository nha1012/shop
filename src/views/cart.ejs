<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Giỏ hàng</title>
    <link
      rel="stylesheet"
      href="//fonts.googleapis.com/css?family=Roboto:400,500"
    />
    <link rel="stylesheet" href="styles/style.css" />
    <script src="scripts/uikit.js"></script>
    <script src="scripts/uikit-icons.js"></script>
  </head>
  <body>
    <div class="uk-offcanvas-content">
      <%- include('component/header')%>
      <main>
        <section class="uk-section uk-section-small">
          <div class="uk-container">
            <div class="uk-grid-medium uk-child-width-1-1" uk-grid>
              <div class="uk-text-center">
                <ul class="uk-breadcrumb uk-flex-center uk-margin-remove">
                  <li><a href="/">Trang chủ</a></li>
                  <li><span>Giỏ hàng</span></li>
                </ul>
                <h1 class="uk-margin-small-top uk-margin-remove-bottom">
                  Giỏ hàng
                </h1>
              </div>
              <div>
                <div class="uk-grid-medium" uk-grid>
                  <div class="uk-width-1-1 uk-width-expand@m">
                    <div
                      class="uk-card uk-card-default uk-card-small tm-ignore-container"
                    >
                      <header
                        class="uk-card-header uk-text-uppercase uk-text-muted uk-text-center uk-text-small uk-visible@m"
                      >
                        <div class="uk-grid-small uk-child-width-1-2" uk-grid>
                          <div>Sản phẩm</div>
                          <div>
                            <div
                              class="uk-grid-small uk-child-width-expand"
                              uk-grid
                            >
                              <div>Giá bán</div>
                              <div class="tm-quantity-column">Số lượng</div>
                              <div>Tổng</div>
                              <div class="uk-width-auto">
                                <div style="width: 20px"></div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </header>
                      <div class="uk-card-body">
                        <div
                          class="uk-grid-small uk-child-width-1-1 uk-child-width-1-2@m uk-flex-middle cart-wrapper__js"
                          uk-grid
                        >
                          <!-- Product cell-->
                          <% if(carts){%> <% carts.forEach(function(cartItem){%>
                          <div
                            class="cart-item__page"
                            data-id="<%= cartItem.orderId%>"
                          >
                            <div class="uk-grid-small" uk-grid>
                              <div class="uk-width-1-3">
                                <div class="tm-ratio tm-ratio-4-3">
                                  <a
                                    class="tm-media-box"
                                    href="./product/<%= cartItem.productId%>"
                                    ><figure class="tm-media-box-wrap">
                                      <img
                                        src="<%= cartItem.product.anhMinhHoa%>"
                                        alt="<%= cartItem.product.tenSanPham%>"
                                      /></figure
                                  ></a>
                                </div>
                              </div>
                              <div class="uk-width-expand">
                                <div class="uk-text-meta"></div>
                                <a
                                  class="uk-link-heading"
                                  href="./product/<%= cartItem.productId%>"
                                  ><%= cartItem.product.tenSanPham%></a
                                >
                              </div>
                            </div>
                          </div>

                          <!-- Other cells-->
                          <div>
                            <div
                              class="uk-grid-small uk-child-width-1-1 uk-child-width-expand@s uk-text-center"
                              uk-grid
                            >
                              <div>
                                <div class="uk-text-muted uk-hidden@m">
                                  Giá bán
                                </div>
                                <div><%= cartItem.product.giaKhuyenMai%> VNĐ</div>
                              </div>
                              <div class="tm-cart-quantity-column">
                                <a
                                  onclick="incrementOrder(-1, '<%= cartItem.productId%>')"
                                  uk-icon="icon: minus; ratio: .75"
                                ></a
                                ><input
                                  class="uk-input tm-quantity-input"
                                  id="product-2"
                                  type="text"
                                  maxlength="3"
                                  value="<%= cartItem.qty%>"
                                /><a
                                  onclick="incrementOrder(+1, '<%= cartItem.productId%>')"
                                  uk-icon="icon: plus; ratio: .75"
                                ></a>
                              </div>
                              <div>
                                <div class="uk-text-muted uk-hidden@m">Tổng</div>
                                <div class="sum-cart__js">
                                  <%= cartItem.tongTien%>
                                </div>
                              </div>
                              <div class="uk-width-auto@s">
                                <a
                                  class="uk-text-danger remove-order__js"
                                  data-id="<%= cartItem.orderId%>"
                                  uk-tooltip="Remove"
                                  ><span uk-icon="close"></span
                                ></a>
                              </div>
                            </div>
                          </div>
                          <%})}%>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="uk-width-1-1 tm-aside-column uk-width-1-4@m">
