        <section class="uk-section uk-section-default uk-section-small">
          <div class="uk-container">
            <div uk-slider>
              <ul
                class="uk-slider-items uk-child-width-1-1 uk-child-width-1-2@s uk-child-width-1-5@m uk-grid"
              >
                <li>
