            <div
              class="uk-grid-small uk-child-width-1-2 uk-child-width-1-3@s uk-child-width-1-6@m"
              uk-grid
            >


								<% if(danhMucSanPhams && danhMucSanPhams.length ){ %> <%
									danhMucSanPhams.forEach(function(danhMucSanPham){ %>
										<div>
											<a
												class="uk-link-muted uk-text-center uk-display-block uk-padding-small uk-box-shadow-hover-large"
												href="subcategory"
												><div class="tm-ratio tm-ratio-4-3">
													<div class="tm-media-box">
														<figure class="tm-media-box-wrap">
															<img
																class="item-brand"
																src="<%= danhMucSanPham.url%>"
																alt="Laptops"
															/>
														</figure>
													</div>
												</div>
												<div class="uk-margin-small-top">
													<div class="uk-text-truncate"><%- danhMucSanPham.tenDanhMuc%></div>
												</div></a
											>
										</div>
								<%})}%>
          </div>
        </section>
        <section class="uk-section uk-section-small">
          <div class="uk-container">
            <h2 class="uk-text-center">Các mặt hàng thịnh hành</h2>
            <div class="uk-card uk-card-default tm-ignore-container">
              <div
                class="uk-grid-collapse uk-child-width-1-3 uk-child-width-1-4@m tm-products-grid"
                uk-grid
              >
                <% if(productsTrending && productsTrending.length ){ %>
                <%productsTrending.forEach(function(product){ %>
                <article class="tm-product-card" data-id="<%= product.productId%>">
                  <div class="tm-product-card-media">
                    <div class="tm-ratio tm-ratio-4-3">
                      <a
                        class="tm-media-box"
                        href="product/<%= product.productId%>"
                        ><div class="tm-product-card-labels">
                          <span class="uk-label uk-label-warning"
                            >Bán chạy nhất</span
                          ><span class="uk-label uk-label-danger"
                            >Đối tác</span
                          >
                        </div>
                        <figure class="tm-media-box-wrap">
                          <img
                            src="<%= product.anhMinhHoa %>"
                            alt='Apple MacBook Pro 15" Touch Bar MPTU2LL/A 256GB (Silver)'
                          /></figure
                      ></a>
                    </div>
                  </div>
                  <div class="tm-product-card-body">
                    <div class="tm-product-card-info">
                      <div class="uk-text-meta uk-margin-xsmall-bottom">
                        <%- product.danhMucSanPham.tenDanhMuc %>
                      </div>
                      <h3 class="tm-product-card-title">
                        <a
                          class="uk-link-heading"
                          href="product/<%= product.productId%>"
                        >
                          <%- product.tenSanPham %>
                        </a>
                      </h3>
                      <ul
